@model  HouseRentalMVCProject.Models.RegisterTbl
@{
    ViewBag.Title = "Login";
}
<center>
        @using (Html.BeginForm("Login", "Home"))
        {
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
            <table borger="1" backColor="Yellow">
                <tr>
                    <td align="center" colspan="2">LogIn</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => m.email)</td>
                    <td>@Html.TextBoxFor(m => m.email)</td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(m => m.pass)</td>
                    <td>@Html.TextBoxFor(m => m.pass)</td>
                </tr>
                <tr>
                    <td colspan="2" align="center">
                        <input type="Submit" class="btn-sm" value="Login" />

                    </td>
                </tr>
            </table>
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
        }
    </center>

